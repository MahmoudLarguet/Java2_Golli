package view;
import javax.swing.*;
import java.awt.*;
public class TabMenu extends JTabbedPane {
	private JPanel welcome,employeManagement,holidayManagement,holidayCalendar,gestionEmpl;
	private JSplitPane phManagement;
	public TabMenu() {
		super(JTabbedPane.TOP);
		welcome = new Welcome();
		phManagement = new JSplitPane();
		employeManagement = new JPanel();
		holidayManagement = new JPanel();
		holidayCalendar = new JPanel();
		gestionEmpl = new GestionEmp();
		add("Welcome",welcome);   
		add("Gestion Employes",gestionEmpl);
		
	}
}
